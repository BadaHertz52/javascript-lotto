{
  "rules": {
    "max-depth": ["error", 1],
    "max-lines-per-function": ["error", { "max": 10 }],
    "max-params": ["error", 2]
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "prettier"]
}
